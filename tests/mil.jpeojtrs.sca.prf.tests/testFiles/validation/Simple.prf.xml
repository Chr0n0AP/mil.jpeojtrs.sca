  </struct>
  <struct id="redundantKind" mode="readwrite" name="redundantKind">
    <simple id="redundantKind::redundantKindWarning" name="redundantKindWarning" type="string">
      <description>This simple triggers a warning because it has a kind type, but isn't top-level</description>
      <kind kindtype="property"/>
    </simple>
    <configurationkind kindtype="property"/>
  </struct>
  <simple id="messageWarning" mode="readwrite" name="messageWarning" type="string">
    <kind kindtype="message"/>
    <action type="external"/>
  </simple>
</properties>
